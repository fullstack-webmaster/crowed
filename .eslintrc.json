{
    "parser": "babel-eslint",
    "plugins": [
        "eslint-plugin-react"
    ],
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "es6": true
    },
    "rules": {
        "no-console": 2,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 1,
        "require-jsdoc": 1,
        "valid-jsdoc": 1
    }
}